<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="AdministracionsPU" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>administracion</jta-data-source>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <class>com.entity.pojos.Cargo</class>
    <class>com.entity.pojos.Empleados</class>
    <class>com.entity.pojos.Administrador</class>
    <class>com.entity.pojos.Reporte</class>
    <class>com.entity.pojos.Login</class>
    <class>com.entity.pojos.Loginadmin</class>
    <class>com.entity.pojos.Direccion</class>
    <class>com.entity.pojos.ReporteSemanal</class>
    <class>com.entity.pojos.Semanas</class>
    <class>com.entity.pojos.ControladorPeticiones</class>
    <class>com.entity.pojs.ConverterTime</class>
    <properties>
      <property name="jakarta.persistence.schema-generation.database.action" value="none"/>
    </properties>
  </persistence-unit>
</persistence>
